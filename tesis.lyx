\begin_layout Standard

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\noun off
\color none
\lang english
\begin_inset Formula \[
v_{i}=\{v_{1}^{i},v_{2}^{i},\ldots,v_{k-x}^{i},v_{q-y+1}^{j},v_{q-y+2}^{j},\ldots,v_{q}^{j}\}\]

\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset Formula \[
v_{j}=\{v_{1}^{j},v_{2}^{j},\ldots,v_{q-y}^{j},v_{k-x+1}^{i},v_{k-x+2}^{j},\ldots,v_{q}^{j}\}\]

\end_inset


\end_layout

